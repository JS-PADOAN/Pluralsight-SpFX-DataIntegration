{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "filter-news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/filterNews/FilterNewsWebPart.js",
          "manifest": "./src/webparts/filterNews/FilterNewsWebPart.manifest.json"
        }
      ]
    },
    "news-sentiment-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/newsSentiment/NewsSentimentWebPart.js",
          "manifest": "./src/webparts/newsSentiment/NewsSentimentWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-2.1.1.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "FilterNewsWebPartStrings": "lib/webparts/filterNews/loc/{locale}.js",
    "NewsSentimentWebPartStrings": "lib/webparts/newsSentiment/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}